{
  "name": "universal-ssl-validator",
  "version": "1.0.0",
  "description": "Advanced SSL validation system with AI-powered security and eco-friendly features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "docs": "typedoc src/",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nabz0r/universal-ssl-validator.git"
  },
  "keywords": [
    "ssl",
    "security",
    "validation",
    "ai",
    "eco-friendly"
  ],
  "author": "nabz0r <nabz0r@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabz0r/universal-ssl-validator/issues"
  },
  "homepage": "https://github.com/nabz0r/universal-ssl-validator#readme",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.10.0",
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "node-forge": "^1.3.1",
    "express-rate-limit": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2"
  }
}